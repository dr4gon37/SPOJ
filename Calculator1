#include <iostream>
using namespace std;


int main()
{
	int n1, n2;
	char op;
	while (cin >> op >> n1 >> n2) {
		if (op == '+')
		{
			cout << n1 + n2 << endl;
		}
		else if (op == '-')
		{
			cout << n1 - n2 << endl;
		}
		else if (op == '*')
		{
			cout << n1 * n2 << endl;
		}
		else if (op == '/')
		{
			cout << n1 / n2 << endl;
		}
		else if (op == '%')
		{
			cout << n1 % n2 << endl;
		}
	}

}
